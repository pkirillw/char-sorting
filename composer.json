{
    "name": "pkirillw/charSorting",
    "type": "library",
    "description": "Класс для сортировка букв в слове. (Тестовое задание SimUni)",
    "keywords": [
        "pkirillw",
        "charSorting"
    ],
    "homepage": "https://github.com/pkirillw/charSorting",
    "license": "MIT",
    "authors": [
        {
            "name": "PKirillW",
            "email": "pkirillw@gmail.com",
            "homepage": "https://github.com/pkirillw",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "pkirillw\\charSorting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pkirillw\\charSorting\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
